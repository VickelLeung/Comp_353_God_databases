
CREATE TABLE Student(
    SID INT AUTO_INCREMENT PRIMARY KEY,
    Name CHAR(20),
    Gender CHAR(6),
    Email VARCHAR(50)
);
 
CREATE TABLE Project(
    PID INT AUTO_INCREMENT PRIMARY KEY,
    Name CHAR(30) NOT NULL
);

CREATE TABLE TEAMS(
    TID INT AUTO_INCREMENT PRIMARY KEY,
    LeaderID INT DEFAULT "NULL",
    NoOfMembers INT NOT NULL,
    CHECK(NoOfMembers <=4),
    UNIQUE(LeaderID)
);

CREATE TABLE Members(
    SID INT AUTO_INCREMENT,
    TID INT AUTO_INCREMENT,
    DateJoined VAR(20) NOT NULL,
    Role VAR(20) NOT NULL,
    PRIMARY KEY(SID, TID)

);

CREATE TABLE Demos(
    SID INT AUTO_INCREMENT,
    TID INT AUTO_INCREMENT,
    date VAR(20) NOT NULL,
    time VAR(20) NOT NULL,
    Grade VAR(1) NOT NULL,
    PRIMARY KEY(SID, TID)

);
